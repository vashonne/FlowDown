    "NSLocalNetworkUsageDescription" : {
      "comment" : "Privacy - Local Network Usage Description",
      "extractionState" : "extracted_with_value",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "FlowDown requires LAN access for interact with local service providers. We do not collect any information from your LAN"
          }
        },
        "zh-Hans" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "我们需要本地网络访问权限来连接到你所请求的服务提供商。我们不从你的局域网内收集任何信息。"
          }
        }
      }
    },
    "NSLocationUsageDescription" : {
      "comment" : "Privacy - Location Usage Description",
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "FlowDown requires location permissions to provide location-based assistance when you request it. For example, when you ask \"Find coffee shops near me\" or \"How long will it take to get to my next meeting,\" FlowDown will access your current location to provide relevant recommendations or travel times. Your location is only accessed when needed for specific requests and is not stored by FlowDown. This data may be sent to your chosen service provider if using a cloud-based AI model."
          }
        },
        "zh-Hans" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "我们需要位置权限以便在您请求时提供基于位置的帮助。例如，当您询问\"查找附近的咖啡店\"或\"到达下一个会议需要多长时间\"时，将访问您的当前位置以提供相关建议或行程时间。您的位置仅在特定请求需要时被访问，而不会被存储。如果使用基于云的 AI 模型，这些数据可能会发送给您选择的服务提供商。"
          }
        }
      }
    },
    "NSLocationWhenInUseUsageDescription" : {
      "comment" : "Privacy - Location When In Use Usage Description",
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "new",
            "value" : "FlowDown needs this permission to access your location when required by AI. Your data will be available for the query and may be uploaded to your service provider."
          }
        },
        "zh-Hans" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "需要此权限以便在 AI 需要时访问您的位置。您的数据将用于查询，并可能上传至您的服务提供商。"
          }
        }
      }
    },
